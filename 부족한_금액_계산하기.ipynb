        "def solution(price, money, count):\n",
        "    for i in range(1,count+1):\n",
        "        money = money - price*i\n",
        "    if money >= 0:\n",
        "        return 0\n",
        "    else : return abs(money)\n",
        "\n",
        "print(solution(3,20,4))"
      ]
