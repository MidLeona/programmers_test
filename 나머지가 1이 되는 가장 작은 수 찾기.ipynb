        "def solution(n):\n",
        "    x = 0\n",
        "    for i in range(1,n):\n",
        "        if n%i == 1:\n",
        "            x = i\n",
        "            if x !=0:\n",
        "                break\n",
        "    return x\n",
        "\n",
        "print(solution(10))\n",
        "print(solution(12))"
      ]
